menor=0
maior=0
vet=[]
n=int(inpu(‘qual sera o tamanho do seu vetor?’))
for i in range (0,n):
	vet.append(input(‘insira um numero:’))
	if i==0:
		maior=menor=vet[i]
	else:
		if vet[i]>maior:
			maior=vet[i]
		if vet[i]<menor:	
			menor=vet[i]
print(‘voce digitou os valores:’, vet[])
print(‘o maior valor foi:’, maior)
print (‘o menor valor foi:’, menor)
